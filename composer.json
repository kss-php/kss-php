{
    "name": "kss-php/kss-php",
    "description": "A PHP implementation of KSS: a methodology for documenting CSS and generating styleguides",
    "keywords": [
        "kss",
        "styleguide",
        "css documentation"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "symfony/finder": "^6.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2.25"
    },
    "authors": [
        {
            "name": "Russell Ahlstrom",
            "email": "russell.ahlstrom@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kss\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "8.0.2"
        }
    }
}
